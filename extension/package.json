{
  "name": "voice-to-code",
  "displayName": "Voice to Code",
  "description": "Convert voice commands to Python code in VSCode",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:voice-to-code.startVoiceCommand",
    "onCommand:voice-to-code.login",
    "onCommand:voice-to-code.logout"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "voice-to-code.startVoiceCommand",
        "title": "Start Voice Command"
      },
      {
        "command": "voice-to-code.login",
        "title": "Voice to Code: Login"
      },
      {
        "command": "voice-to-code.logout",
        "title": "Voice to Code: Logout"
      }
    ],
    "keybindings": [
      {
        "command": "voice-to-code.startVoiceCommand",
        "key": "ctrl+shift+v",
        "mac": "cmd+shift+v"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "jsonwebtoken": "^9.0.0"
  }
}
